class Solution {
    public int[] sortedSquares(int[] a) {

      
        int n=a.length;
         for(int i=0;i<n;i++){
            a[i]=a[i]*a[i];
        }

        for(int i=0;i<n-1;i++)
		{
		    int min=i;
			for(int j=i+1;j<n;j++)
			{
				if(a[j]<a[min])
				{
				    min=j;
				}
			    
			}
					int temp=a[i];
					a[i]=a[min];
					a[min]=temp;
        }
       
      return a;
    }
}
        
 
