class Solution {
    public String reversePrefix(String word, char ch) {
        int n=word.indexOf(ch);
       String sub="";
        for(int i=0;i<=n;i++){
          sub=word.charAt(i)+sub;
        }
        if(n==-1){
            return word;
        }
        else{
            for(int i=n+1;i<word.length();i++){
                sub=sub+word.charAt(i);
            }
            return sub;
        }
    }
}
