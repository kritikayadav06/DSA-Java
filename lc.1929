class Solution {
    public int[] getConcatenation(int[] nums) {
        int n=nums.length;
        int a[]= new int[n*2];
        for(int i=0;i<a.length;i++){
            if(i<n){
                a[i]=nums[i];
            }
            else{
                a[i]=nums[i-n];
            }
        }
        return a;
        
    }
}
